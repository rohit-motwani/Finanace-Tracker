{
    "version": 2,
    "builds": [
      {
        "src": "finance_tracker/wsgi.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "static/$1"
      },
      {
        "src": "/media/(.*)",
        "dest": "media/$1"
      },
      {
        "src": "/(.*)",
        "dest": "finance_tracker/wsgi.py"
      }
    ]
  }
  